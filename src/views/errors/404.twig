{% extends 'layouts/errors.twig' %}

{% block title %}Page Not Found{% endblock %}

{% block errorCode %}404{% endblock %}

{% block titleError %}We couldn't find the page...{% endblock %}

{% block message %}
    Sorry, but the page you are looking for was either not found or does not exist.
    Try refreshing the page or go back to the previous page.
{% endblock %}

{% block trace %}
    {% if showTrace %}
        {% embed 'sections/errors/trace.twig' %}
            {% block traceMessage %}
                {{ trace }}
            {% endblock %}
        {% endembed %}
    {% endif %}
{% endblock %}